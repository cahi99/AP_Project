#: include 'productos.kv'
#: include 'mipedido.kv'
<Screen1@Screen>
    name: 'Screen1'
    NavigationLayout:
        ScreenManager:
            Screen:
                BoxLayout:
                    orientation: "vertical"
                    MDToolbar:
                        title: "Distribuidores"
                        left_action_items:[['menu',lambda x: nav_drawer.toggle_nav_drawer()]]
                        elevation:5
                    ScrollView:
                        MDList:
                            id:wig
                BoxLayout:
                    padding:'10dp'
                    spacing:'10dp'
                    MDFloatingActionButton:
                        icon: "plus"
                        elevation_normal: 12 
                        md_bg_color: app.theme_cls.primary_color 
                        on_press: root.file_manager_open()
        MDNavigationDrawer:
            id: nav_drawer
            BoxLayout:
                orientation:'vertical'
                spacing: '6dp'
                padding: '6dp'
                Image:
                    source: 'Imagenes/2.jpg'
                MDLabel:
                    text: 'Mi Tienda'
                    font_style: 'Subtitle1'
                    size_hint_y: None
                    height: self.texture_size[1]
                ScrollView:
                    MDList:
                        OneLineIconListItem:
                            text: 'Distribuidores'
                            on_press: app.root.current='Screen1'
                            IconLeftWidget:
                                icon: 'store'
                        OneLineIconListItem:
                            text: 'Mis Pedidos'
                            IconLeftWidget:
                                icon: 'cart'
<Lista>:
    id: objetito
    on_press:root.change(self.text)
